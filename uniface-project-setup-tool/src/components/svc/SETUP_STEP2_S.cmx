;------------------------------------------------------------------
; Import the Uniface defaults
;------------------------------------------------------------------
entry lpImportUnifaceDefaults
;------------------------------------------------------------------
params
	Struct pMessages : INOUT
endparams

variables
	Boolean vImportDefaults
	String  vDefaultsPath
endvariables

	vImportDefaults = $options$-&gt;"config-file"-&gt;"import-defaults"
	if(!vImportDefaults)
		;The config doesn't say to import the defaults, so just return out
		return 0
	endif

	pMessages-&gt;"IMPORT_DEFAULTS" = $newstruct
	pMessages-&gt;"IMPORT_DEFAULTS"-&gt;"DESC" = "Importing Uniface Defaults"

	putmess "Import Uniface defaults"

	vDefaultsPath = $concat($options$-&gt;"uniface"-&gt;"UNIFACE_PATH", "uniface\misc\udefault.xml")
	$1 = $ude("import", "misc", vDefaultsPath)
	if($status &lt; 0)
		pMessages-&gt;"IMPORT_DEFAULTS"-&gt;"STATUS" = "FAILED"			
		return 0
	endif

	pMessages-&gt;"IMPORT_DEFAULTS"-&gt;"STATUS" = "SUCCESS"

	return 0

end ;lpImportUnifaceDefaults


;------------------------------------------------------------------
;Import the Uniface source code
;------------------------------------------------------------------
entry lpImportSourceCode
;------------------------------------------------------------------
params
	Struct pMessages : INOUT
endparams

variables
	Boolean vDoImport
	String  vMessages
endvariables

	vDoImport = $options$-&gt;"config-file"-&gt;"do-import"
	if(!vDoImport)
		;The config doesn't say to import any Uniface source code, so just return out
		return 0
	endif

	pMessages-&gt;"IMPORT_SOURCE" = $newstruct
	pMessages-&gt;"IMPORT_SOURCE"-&gt;"DESC" = "Importing Source"

	putmess "Import Uniface source"

	activate "VREPO_IMPORT".importFromPath("PATH=.\src&uSEP;FORCE_IMPORT=1", vMessages)
	if($status &lt; 0)
		pMessages-&gt;"IMPORT_SOURCE"-&gt;"STATUS"     = "FAILED"
		pMessages-&gt;"IMPORT_SOURCE"-&gt;"ERROR_DESC" = "Source import from path failed with status: %%$status%%%"
		putmess vMessages
		return 0
	endif

	pMessages-&gt;"IMPORT_SOURCE"-&gt;"STATUS" = "SUCCESS"

	return 0

end ;lpImportSourceCode


