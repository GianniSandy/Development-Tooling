;---------------------------------------------------------------------
;Run table creation scripts
; This is old version running ALL sql statements toward DEF
; Substituted with lpRunTableScripts(Messages, Path)
; See below...
;---------------------------------------------------------------------
entry lpRunTableCreateScripts
;---------------------------------------------------------------------
params
	Struct pMessages : INOUT
endparams

variables
	String  vScriptDir
	String  vScripts
	String  vScript
	String  vSql
	String  vStatements
	String  vStatement
endvariables

	pMessages-&gt;"TABLE_CREATE" = $newstruct
	pMessages-&gt;"TABLE_CREATE"-&gt;"DESC" = "Running Table Create Scripts"

	putmess "Run table creation scripts"

	vScriptDir = ".\sql\"
	vScripts = $ldirlist(vScriptDir, "FILE")
	getitem vScript, vScripts, 1
	while(vScript != "")

		lfileload $concat(vScriptDir, vScript), vSql

		vStatements = lpSplitSql(vSql)
		getitem vStatement, vStatements, 1
		while(vStatement != "")	

			sql vStatement, "DEF"

			delitem vStatements, 1
			getitem vStatement, vStatements, 1
		endwhile

		delitem vScripts, 1
		getitem vScript, vScripts, 1
	endwhile

	commit

	pMessages-&gt;"TABLE_CREATE"-&gt;"STATUS" = "SUCCESS"

	return 0

end ;lpRunTableCreateScripts


;---------------------------------------------------------------------
entry lpRunTableScripts
;---------------------------------------------------------------------
; Execute a SQL script on path pPath if was copied to .\sql\
;---------------------------------------------------------------------
params
	Struct pMessages : INOUT
	String pPath     : IN
endparams

variables
	String  vScriptDir
	String  vScript
	String  vSql
	String  vStatements
	String  vStatement
	Numeric vCount
	Numeric vNumberOfDataImports
	Numeric vWarning
endvariables

	pMessages-&gt;"SCRIPT_EXEC_%%pPath%%%" = $newstruct
	pMessages-&gt;"SCRIPT_EXEC_%%pPath%%%"-&gt;"DESC" = "Running Table Scripts on %%pPath%%% path"

	putmess "Exec scripts, path %%pPath%%%"

	vScriptDir = ".\sql\"
	vWarning = 0

	vCount               = 1
	vNumberOfDataImports = $options$-&gt;"config-file"-&gt;"sql-templates-%%pPath%%%"-&gt;*-&gt;$collsize
	while(vCount &lt;= vNumberOfDataImports)

		vScript = $options$-&gt;"config-file"-&gt;"sql-templates-%%pPath%%%"-&gt;*{vCount}-&gt;"to"

		if (vScript[1:$length(vScriptDir)] == vScriptDir &amp; $fileexists(vScript))

			lfileload vScript, vSql

			vStatements = lpSplitSql(vSql)
			getitem vStatement, vStatements, 1
			while(vStatement != "")	

				sql vStatement, pPath
				if ($status &lt; 0) vWarning = 1

				delitem vStatements, 1
				getitem vStatement, vStatements, 1

			endwhile
			commit

		endif
		vCount = vCount + 1

	endwhile

	if (vNumberOfDataImports == 0)
		pMessages-&gt;"SCRIPT_EXEC_%%pPath%%%"-&gt;"STATUS" = "NOTHING TO DO"
	else
		if (vWarning == 0)
			pMessages-&gt;"SCRIPT_EXEC_%%pPath%%%"-&gt;"STATUS" = "SUCCESS"
		else
			pMessages-&gt;"SCRIPT_EXEC_%%pPath%%%"-&gt;"STATUS" = "WARNING(S)"
		endif
	endif

	return 0

end ;lpRunTableCreateScripts


;-------------------------------------------------------------------------
; Loop over any data import declarations and import the files into $DEF
;-------------------------------------------------------------------------
entry lpImportAppData
;-------------------------------------------------------------------------
params
	Struct pMessages : INOUT
endparams

variables
	Numeric vCount
	Numeric vNumberOfDataImports
	String  vLocation
	String  vPath
endvariables

	pMessages-&gt;"IMPORT_DATA" = $newstruct
	pMessages-&gt;"IMPORT_DATA"-&gt;"DESC" = "Importing Data"

	putmess "Import application data"

	vCount               = 1
	vNumberOfDataImports = $options$-&gt;"config-file"-&gt;"data-import-files"-&gt;*-&gt;$collsize
	while(vCount &lt;= vNumberOfDataImports)

		vLocation = $options$-&gt;"config-file"-&gt;"data-import-files"-&gt;*{vCount}-&gt;"location"
		;TODO: Throw an error if this isn't found

		vPath = $options$-&gt;"config-file"-&gt;"data-import-files"-&gt;*{vCount}-&gt;"path"
		;TODO: Throw an error if this isn't found

		call lpImportData(vLocation, vPath)
		if($status &lt; 0)
			pMessages-&gt;"IMPORT_DATA"-&gt;"STATUS" = "FAILED"
			return 0
		endif

		vCount = vCount + 1

	endwhile

	pMessages-&gt;"IMPORT_DATA"-&gt;"STATUS" = "SUCCESS"

	return 0

end ;lpImportAppData


;-------------------------------------------------------------------------
; Create project shortcuts
;-------------------------------------------------------------------------
entry lpCreateShortcuts
;-------------------------------------------------------------------------
params
	Struct pMessages : INOUT
endparams

variables
	Numeric vCount
	Numeric vNumberOfShortcuts
	String  vName
	String  vTarget
	String  vArgs
	String  vSetupCreateFormProperties
	String  vZipFileName
	String  vSetupCreateFormPath
	String  vProjectSetupRoot
	String  vExePath
	String  vShortcutOutput
endvariables

	pMessages-&gt;"CREATE_SHORTCUTS" = $newstruct
	pMessages-&gt;"CREATE_SHORTCUTS"-&gt;"DESC" = "Creating Shortcuts"

	vCount             = 1
	vNumberOfShortcuts = $options$-&gt;"config-file"-&gt;"shortcuts"-&gt;*-&gt;$collsize

	if(vNumberOfShortcuts &gt; 0)
		filedump "Name,Target,Args%%^", "shortcut_config.txt"
	endif

	while(vCount &lt;= vNumberOfShortcuts)

		vName = $options$-&gt;"config-file"-&gt;"shortcuts"-&gt;*{vCount}-&gt;"name"
		;TODO: Throw an error if this isn't found

		vTarget = $options$-&gt;"config-file"-&gt;"shortcuts"-&gt;*{vCount}-&gt;"target"
		;TODO: Throw an error if this isn't found

		vArgs = $options$-&gt;"config-file"-&gt;"shortcuts"-&gt;*{vCount}-&gt;"args"
		;TODO: Throw an error if this isn't found

		; vArgs = $replace(vArgs, 1, "${debugport}", $options$-&gt;"uniface"-&gt;"UDBG_PORT", -1)

		vName = lpSubstituteVariables(vName)
		vTarget = lpSubstituteVariables(vTarget)
		vArgs = lpSubstituteVariables(vArgs)

		;Create shortcut config line
		filedump/append "%%vName%%%,%%vTarget%%%,%%vArgs%%%%%^", "shortcut_config.txt"

		vCount = vCount + 1

	endwhile

	;Run shortcut creator
	vSetupCreateFormProperties = $fileproperties("$RES:frm\setup_create.frm")
	vZipFileName = $item("ZIPFILENAME", vSetupCreateFormProperties)
	if(vZipFileName != "")
		;We're running from a UAR
		vProjectSetupRoot = $item("FULLPATH", $fileproperties("%%vZipFileName%%%\..\", "FULLPATH"))
	else
		;We're running from an unpacked resource directory
		vSetupCreateFormPath = $item("FULLPATH", vSetupCreateFormProperties)
		vProjectSetupRoot    = $item("FULLPATH", $fileproperties("%%vSetupCreateFormPath%%%\..\..\..\", "FULLPATH"))
	endif

	vExePath = $concat(vProjectSetupRoot, "3gl\UnifaceShortcutCreator.exe")

   if ($scan(vExePath, " ") &gt; 0) vExePath = "%%"%%vExePath%%%%%""

	activate "OS".commandOut("%%vExePath%%% --file shortcut_config.txt –v", vShortcutOutput)

	;Clean up the shortcut config file
	filedelete "shortcut_config.txt"

	pMessages-&gt;"CREATE_SHORTCUTS"-&gt;"STATUS" = "SUCCESS"

	return 0

end ;lpCreateShortcuts


;---------------------------------------------------------
entry lpSplitSql
;---------------------------------------------------------
returns String

params
	String pSql : IN
endparams

variables
	String  vStatement
	String  vRemainingSql
	String  vStatementList
	Numeric vResult
endvariables

	vRemainingSql = pSql
	vResult = 1

	while(vResult &gt; 0)

		vResult = $split(vRemainingSql, 1, ";", vStatement, vRemainingSql)
		if(vStatement != "")
			vStatement = $rtrim(vStatement, " ")
			vStatement = $ltrim(vStatement, " ")
			vStatement = $concat(vStatement, ";")

			putitem vStatementList, -1, vStatement
		endif

	endwhile

	return vStatementList

end ;lpSplitSql


;---------------------------------------------------------
entry lpImportData
;---------------------------------------------------------
params
	String pLocation : IN
	String pPath     : IN
endparams

variables
	Numeric vPos
	String  vFolderPart
	String  vFilePart
	String  vLocation
	String  vFileList
	String  vFile
	String  vFullFilePath
	Numeric vReturn
	String  vCopyReturnContext
endvariables

	;Get the location of the last backslash
	vPos = $rscan(pLocation, "\")

	vFolderPart = pLocation[1:vPos]
	vFilePart   = pLocation[vPos + 1]

	vFilePart = $replace(vFilePart, 1, "*", "&uALL;", -1)

	vLocation = $concat(vFolderPart, vFilePart)

	vFileList = $ldirlist(vLocation, "FILE")

	forlist vFile in vFileList

		vFullFilePath = $concat(vFolderPart, vFile)

		vReturn = $ude("copy", "misc", vFullFilePath, "%%pPath%%%:")
		vCopyReturnContext = $procReturnContext

	endfor

	return 0

end ;lpImportData
</DAT>
<DAT name="FORMPIC" xml:space='preserve'>
 &uFRM;TYP=E&uSEP;NAM=DUMMY.DUMMY&uSEP;WID=20&uSEP;HEI=1&uSEP;HOC=20&uSEP;VOC=1&uFRM;</DAT>
<DAT name="SFUNC" xml:space='preserve'>;-------------------------------------------------------------------
; Called from start-up shell. Handles batch setup tasks that should
; be done after the setupProject operation is done creating things
; like the dbms.asn that should be used by this operation.
;
; pSetupFile is a struct dump containing all the configuration that
; should be used. The configuration is everything found in the
; project setup XML file along with any input captured from the
; user in the first stage of the setup process.
;-------------------------------------------------------------------
operation doSetup
;-------------------------------------------------------------------
params
	String pSetupFile : IN
endparams

variables
	Struct  vMessages
	Struct  vSetupStruct
	String  vMessagesJson
endvariables

	;Struct for passing messages back to the main process. Will get dumped as a file and read back by the main process so
	;that progress or errors can be shown to the user.
	vMessages = $newstruct

	xmlToStruct vSetupStruct, pSetupFile
	if($status &lt; 0)
		vMessages-&gt;"ERROR" = "Failed to load project settings (status: %%$status%%%)"
	endif

	$options$ = vSetupStruct-&gt;"local-setup"

	;------------------------------------------------------------------
	;Run the various step 2 setup tasks
	;------------------------------------------------------------------
	call lpRunTableScripts(vMessages, "IDF")
	call lpImportUnifaceDefaults(vMessages)
	call lpImportMetaDict(vMessages)
	call lpImportSourceCode(vMessages)
	call lpRunTableScripts(vMessages, "SYS")
	call lpDoCompile(vMessages)
;	call lpRunTableCreateScripts(vMessages)		; Substituted from 3 times call lpRunTableScripts()
	call lpRunTableScripts(vMessages, "DEF")
	call lpImportAppData(vMessages)
	call lpCreateShortcuts(vMessages)

	;Dump out any logged messages so they can be displayed by the parent
	;Uniface process
	structToJson vMessagesJson, vMessages
	filedump vMessagesJson, "step_2_messages.json"

	return 0

end ;doSetup
