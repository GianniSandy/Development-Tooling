<DAT name="GENERAL" xml:space='preserve'>;---------------------------------------
entry lpConcatPaths
;---------------------------------------
returns String

params
	String pFilePath : IN
	String pFolder   : IN
endparams

variables
	Numeric vLength
	String  vNewFilePath
endvariables

	if(pFolder != "")
		vLength = $length(pFilePath)
		if(pFilePath[vLength] != "\" &amp; pFolder[1:1] != "\")
			vNewFilePath = $concat(pFilePath, "\", pFolder)
		elseif(pFilePath[vLength] == "\" &amp; pFolder[1:1] == "\")
			vLength = vLength - 1
			vNewFilePath = $concat(pFilePath[1:vLength], pFolder)
		else
			vNewFilePath = $concat(pFilePath, pFolder)
		endif
	else
		vNewFilePath = pFilePath
	endif

	$status = 0
	return vNewFilePath

end ;lpConcatPaths


;------------------------------------
entry lpCopyFile
;------------------------------------
params
	String pSource      : IN
	String pDestination	: IN
endparams

	if($lfileexists(pDestination) == 1)
		lfiledelete pDestination
	endif

	lfilecopy pSource, pDestination

	return 0

end ;lpCopyFile


;------------------------------------
entry lpDeleteFile
;------------------------------------
params
	String	pFile	: IN
endparams

	if( $lfileexists(pFile) )
		lfiledelete(pFile)
	endif

end ;lpDeleteFile


;------------------------------------
entry lpCreateDirectory
;------------------------------------
params
	String	pDirectory : IN
endparams

	call lpDeleteDirectory(pDirectory)
	ldircreate pDirectory

	return 0

end ;lpCreateDirectory


;------------------------------------
entry lpDeleteDirectory
;------------------------------------
params
	String	pDirectory	:IN
endparams

variables
	String	vFileList
	String	vFile
	String	vFullPath
endvariables

	if( $lfileexists(pDirectory) )

		vFileList = $ldirlist(pDirectory)
		getitem vFile, vFileList, 1
		while($status &gt; 0)

			vFullPath = lpConcatPaths(pDirectory, vFile)
	
			if($fileproperties(vFullPath, "FILETYPE") == "FILETYPE=DIR")
				call lpDeleteDirectory(vFullPath)
			elseif($fileproperties(vFullPath, "FILETYPE") == "FILETYPE=FILE")
				lfiledelete(vFullPath)
			endif

			delitem vFileList, 1
			getitem vFile, vFileList, 1
		endwhile

		ldirdelete(pDirectory)
	endif

	return 0

end ;lpDeleteDirectory</DAT>
