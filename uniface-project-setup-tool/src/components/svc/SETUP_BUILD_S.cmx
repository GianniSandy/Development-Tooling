<DAT name="INIT" xml:space='preserve'>;----------------------------------------------------------------------
; Clean up any old builds and create the folder structure
;----------------------------------------------------------------------
call lpCreateDirectory(".\build")
call lpCreateDirectory(".\build\ProjectSetup")
call lpCreateDirectory(".\build\ProjectSetup\3gl")
call lpCreateDirectory(".\build\ProjectSetup\asn")
call lpCreateDirectory(".\build\ProjectSetup\log")
call lpCreateDirectory(".\build\ProjectSetup\ext")
;TODO - It would be nice if we didn't have to manually create all this, but copying a folder to another folder
;       will require creating some decent common file handling logic.
call lpCreateDirectory(".\build\ProjectSetup\project-templates")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\base-project")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\base-project\asn")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\base-project\asn\templates")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\base-project\src")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\asn")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\asn\templates")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\src")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\web")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\web\WEB-INF")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\web\WEB-INF\error_en")
call lpCreateDirectory(".\build\ProjectSetup\project-templates\web-project\web\WEB-INF\templates")


;----------------------------------------------------------------------
; Compile all the objects required for the application
;----------------------------------------------------------------------
$1 = $ude("compile", "application", "SETUP",         "prcadditional=true")
$1 = $ude("compile", "form",        "SETUP_&uALL;",      "prcadditional=true")
$1 = $ude("compile", "service",     "SETUP_INIT_S",  "prcadditional=true")
$1 = $ude("compile", "service",     "SETUP_MAIN_S",  "prcadditional=true")
$1 = $ude("compile", "service",     "SETUP_STEP2_S", "prcadditional=true")
$1 = $ude("compile", "signature",   "OS",            "prcadditional=true")
$1 = $ude("compile", "variables",   "&uALL;",            "prcadditional=true")


;----------------------------------------------------------------------
; Copy other resources required by the built project
;---------------------------------------------------------------------
filecopy  ".\3gl\CommandLine.dll",            ".\build\ProjectSetup\3gl\CommandLine.dll"
